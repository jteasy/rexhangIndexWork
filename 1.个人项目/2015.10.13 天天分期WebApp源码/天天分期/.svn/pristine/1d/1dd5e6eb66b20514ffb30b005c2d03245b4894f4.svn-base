<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <meta name="format-detection" content="telephone=no,email=no,date=no,address=no">
    <title>常见问题</title>
    <link rel="stylesheet" type="text/css" href="../../css/api.css" />
    <link rel="stylesheet" type="text/css" href="../../css/common.css" />
<style type="text/css" media="screen">
	*{
		margin:0;
		padding:0;
	}
	html{
		width:100%;
		height:100%;
		overflow-x: hidden;
		-webkit-text-size-adjust:none;  
	}
	body{
		width:100%;
		height:100%;
		background:#EFEFF2;
		overflow-x: hidden;
	}
	body::-webkit-scrollbar{
	    display: none;
	}
	body,button, input, select, textarea {
	     font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif;
	}
	img{
	  border:0;
	}
	.top-divide{
	  width:100%;
	  text-align:center;
	  color:#FFF;
	  font-size:2rem;
	  background: #0DA4FF;
	}
	.fq-logo{
	  width:50%;
	  padding-top: 30%;
	
	}
	.iphone-p {
	  padding: 4% 0 22% 0;
	  font-size: 18px;
	  margin-top: 14px;
	}
	.feedback{
		width:100%;
		margin-top:10px;
		height:60px;
		border-top:1px solid #d8d8d8;
		border-bottom:1px solid #d8d8d8;
		font-size:18px;
		background:#fff;
		line-height:60px;
	}
	.feedback font{
		margin-left:14px;
		color:#666666;
	}
	.feedback span{
		     width: 34px;
		    margin-right: 10px;
		    height: 60px;
		    float: right;
		    background: url(../../image/right_arrow.png) no-repeat;
		    background-size: 40%;
		    background-position: 72% 50%;
	}
	.copyright{
		font-size:14px;
		text-align:center;
		margin:0 auto;
		width:100%;
		margin-top:50px;
		color:#999999;
		line-height:24px;
	}
</style>
   <body>
      <div class="top-divide">
        <img src="../../image/about_logo.png" class="fq-logo">
        <p class="iphone-p"><font id="system"></font>  1.1</p>
      </div>
	    <div class="feedback" tapmode="" onclick="openWin('feedback')"><font>意见反馈</font><span></span></div>
	    <div class="copyright">
	    	<p>Copyright ©2015-2016</p>
	    	<p>西安开图电子商务有限公司</p>
	    </div>
   </body>
<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript">
	apiready=function(){
		if(api.systemType=='android'){
			var system="android";
		}else if(api.systemType=='android'){
			var system="iphone";
		}
		$api.text($api.byId('system'),system);
	}
	/**
	 *打开窗口 
	 */
	var openWin=function($url){
		api.openWin({
	        name:$url,
	        url: $url+'_header.html',
	        bgcolor:"#ffffff",
	        scrollToTop:true,
	        animation:{
	        	type:'movein',
	        	subType:'from_right'
	        }
        });
	}
</script>
</html>